# groups/filter

{
	count: int,
	groups [
				{ id: int, 
				title : string, 
				key: string, 
				count: int,
				art: {
							thumb: url,
							fanart: url, 
							banner: url
						}
				}
			]
		}

# series/seasons
{
	count: int,
	series: [
			{ 	id: int, 
				count_local: int,
				title : string, 
				titles: {
							official: {
											jp: string,
											en: string
										},
							synonym: {
											jp: string,
											en: string
										},
							short: {
											jp: string,
											en: string
										}
						},
				year: int,
				episode: int,
				season: int,
				rating: float,
				//votes: int,
				//my_rating: float,
				datastart: datetime,
				//datastop: datetime,
				plot: string,
				//status: string,
				dateadded: datetime,
				key : url,
				art: {
						thumb: url,
						fanart: url, 
						banner: url
					}
				source: tvshows,
				watched: int
				genre: [ string, string, string ]
				cast: [
									{ 
										actor: string, 
										actorpic: string,
										role: string, 
										roledesc: string,
										rolepic: string 
										},
									{ 
										actor: string, 
										actorpic: string,
										role: string, 
										roledesc: string,
										rolepic: string 
										}
				],
				tag: [ string, string, string ],
			}
			]
		}

# episodes
{
	count: int,
	tag: [ string, string, string ],
	cast: [
									{ 
										actor: string, 
										actorpic: string,
										role: string, 
										roledesc: string,
										rolepic: string 
										},
									{ 
										actor: string, 
										actorpic: string,
										role: string, 
										roledesc: string,
										rolepic: string 
										}
				],
	year: int,
	genre: [ string, string, string ]
	episodes: [
			{ 	id: int, 
				title: string,				
				episode: int,
				season: int,
				rating: float,
				//votes: int,
				//my_rating: float,
				datastart: datetime,
				//datastop: datetime,
				plot: string,
				//status: string,
				dateadded: datetime,
				key : url,
				art: {
						thumb: url,
						fanart: url, 
						banner: url
					}
				watched: int
				media: [
							{
								parts: [
											{
												#stream=1
												{
													id: int,
													type: string,
													VideoCodec: string, 
													width: int,
													height: int,
													duration: int
												},
												#stream=2
												{
													id: int,
													type: string,
													AudioCodec: string,
													AudioLanguage: string,
													AudioChannels: string,
												},
												#stream=3
												{
													id: int,
													type: string,
													Language: string
												}
											}
										]
									}
								]
			}
		]
	}
                              
